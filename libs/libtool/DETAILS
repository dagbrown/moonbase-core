          MODULE=libtool
         VERSION=2.4.7
          SOURCE=$MODULE-$VERSION.tar.gz
   SOURCE_URL[0]=$GNU_URL/$MODULE
   SOURCE_URL[1]=https://ftp.gnu.org/pub/gnu/$MODULE
      SOURCE_VFY=sha256:04e96c2404ea70c590c546eba4202a4e12722c640016c12b9b2f1ce3d481e9a8
